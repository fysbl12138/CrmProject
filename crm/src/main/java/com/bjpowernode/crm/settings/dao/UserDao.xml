<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DID Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjpowernode.crm.settings.dao.UserDao">

    <select id="login" resultType="com.bjpowernode.crm.settings.entity.User">

        select * from tbl_user where loginAct = #{loginAct} and loginPwd = #{loginPwd}

    </select>

    <select id="selectAllUser" resultType="com.bjpowernode.crm.settings.entity.User">

        select * from tbl_user

    </select>

</mapper>